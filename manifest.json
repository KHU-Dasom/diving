{
  "manifest_version": 2,
  "name": "diving",
  "description": "find wiki",
  "version": "0.1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    { 
      "matches": ["http://*/*", "https://*/*"], 
      "js": ["loader.js"] 
    }
  ]
}